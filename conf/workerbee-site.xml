<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>

<!-- Put site-specific property overrides in this file. -->

<configuration>
	<property>
		<name>workerbee.jdbc.driver</name>
		<value>com.mysql.jdbc.Driver</value>
		<description>JDBC driver class.</description>
	</property>

	<property>
		<name>workerbee.jdbc.url</name>
		<value>jdbc:mysql://localhost:3306/workerbee_dev</value>
		<description>JDBC URL.</description>
	</property>

	<property>
		<name>workerbee.jdbc.username</name>
		<value>root</value>
		<description>DB user name.</description>
	</property>

	<property>
		<name>workerbee.jdbc.password</name>
		<value>data1234</value>
		<description>DB user paswsword.</description>
	</property>
</configuration>
